# -*- mode: python ; coding: utf-8 -*-
a = Analysis(
    ['src/chromatopy/__main__.py'],
    pathex=['src'],
    binaries=[],
    datas=[],
    hiddenimports=[],
    hookspath=[],
    hooksconfig={},
    runtime_hooks=[],
    excludes=[],
    noarchive=False,
    optimize=0,
)
pyz = PYZ(a.pure)

exe = EXE(
    pyz,
    a.scripts,
    [],
    exclude_binaries=True,
    name='chromatopy',
    debug=False,
    bootloader_ignore_signals=False,
    strip=False,
    upx=True,
    console=False,
    disable_windowed_traceback=False,
    argv_emulation=False,
    target_arch=None,
    codesign_identity=None,
    entitlements_file=None,
    icon=['/Users/gerard/chromatopy/chromatopy_icon.icns'],
)
coll = COLLECT(
    exe,
    a.binaries,
    a.datas,
    strip=False,
    upx=True,
    upx_exclude=[],
    name='chromatopy',
)
app = BUNDLE(
    coll,
    name='chromatopy-GDGT.app',
    icon='/Users/gerard/Documents/GitHub/chromatoPy/chromatopy_icon.icns',
    bundle_identifier=None,
)
